{
  "name": "un-component-vue",
  "private": true,
  "packageManager": "pnpm@7.15.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "eslint . --fix.",
    "clean:modules": "pnpm -r exec rimraf node_modules && rimraf node_modules",
    "clean:dist": "pnpm -r exec rimraf dist",
    "dev:play": "pnpm --filter @un-component-vue/playground dev",
    "build:ui": "pnpm --filter @un-component-vue/ui build",
    "build:shard": "pnpm --filter @un-component-vue/shard build",
    "build:all": "pnpm run build:shard && npm run build:component"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@turbo/gen": "^1.10.3",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.3.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "eslint": "^8.42.0",
    "eslint-config-custom": "workspace:*",
    "rimraf": "^5.0.1",
    "turbo": "latest",
    "typescript": "^4.9.5",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.6.5"
  }
}
