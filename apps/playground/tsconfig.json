{
  "extends": "tsconfig/dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "jsx": "preserve",
    "composite": true,
    "baseUrl": ".",
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}
